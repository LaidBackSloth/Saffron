//proper implementation of mossy stones
function mossySpecific(input as IItemStack, output as IItemStack, doVine as bool) as void {
	var outName = output.registryName.namespace + "-" + output.registryName.path;
	craftingTable.addShapeless(outName + "_from_moss_clump", output, [input, <item:immersive_weathering:moss_clump>]);
	craftingTable.addShapeless(outName + "_from_moss_block", output, [input, <item:minecraft:moss_block>]);
	if (doVine) {
		craftingTable.addShapeless(outName + "_from_vines", output, [input, <item:minecraft:vine>]);
	}
}

function mossySimple(mod as string, id as string, doVine as bool) as void {
	var outId = "mossy_" + id;
	mossySpecific(<item:${mod}:${id}>, <item:${mod}:${outId}>, doVine);
}

function mossy(mod as string, id as string) as void {
	mossySimple(mod, id, true);
}

