craftingTable.addShaped("terracotta_brick_vertical_slab", <item:clayworks:terracotta_brick_vertical_slab> * 3, [[<item:clayworks:terracotta_brick_slab>], [<item:clayworks:terracotta_brick_slab>], [<item:clayworks:terracotta_brick_slab>]]);

for color in colors {
	craftingTable.remove(<item:clayworks:${color}_terracotta_vertical_slab>);
	craftingTable.addShaped(color + "_terracotta_vertical_slab", <item:clayworks:${color}_terracotta_vertical_slab> * 3, [[<item:clayworks:${color}_terracotta_slab>], [<item:clayworks:${color}_terracotta_slab>], [<item:clayworks:${color}_terracotta_slab>]]);
	craftingTable.remove(<item:clayworks:${color}_terracotta_brick_vertical_slab>);
	craftingTable.addShaped(color + "_terracotta_brick_vertical_slab", <item:clayworks:${color}_terracotta_brick_vertical_slab> * 3, [[<item:clayworks:${color}_terracotta_brick_slab>], [<item:clayworks:${color}_terracotta_brick_slab>], [<item:clayworks:${color}_terracotta_brick_slab>]]);
}
